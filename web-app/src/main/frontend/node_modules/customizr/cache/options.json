{
  "version": "1.4.0",
  "modernizr": "^3",
  "options": {
    "feature-detects": [
      "test/json",
      "test/svg",
      "test/touchevents",
      "test/unicode",
      "test/css/animations",
      "test/css/backgroundsize",
      "test/css/boxshadow",
      "test/css/boxsizing",
      "test/css/checked",
      "test/css/ellipsis",
      "test/css/flexbox",
      "test/css/fontface",
      "test/css/gradients",
      "test/css/lastchild",
      "test/css/mediaqueries",
      "test/css/nthchild",
      "test/css/opacity",
      "test/css/rgba",
      "test/css/scrollbars",
      "test/css/textshadow",
      "test/css/transforms",
      "test/css/transitions",
      "test/elem/unknown",
      "test/svg/asimg",
      "test/svg/inline",
      "test/textarea/maxlength"
    ],
    "options": [
      "setClasses"
    ],
    "minify": false,
    "dest": "../webapp/assets/js/libs/modernizr.js",
    "classPrefix": ""
  }
}