#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../cldr-data-downloader/bin/download.sh" "$@"
  ret=$?
else 
  node  "$basedir/../cldr-data-downloader/bin/download.sh" "$@"
  ret=$?
fi
exit $ret
